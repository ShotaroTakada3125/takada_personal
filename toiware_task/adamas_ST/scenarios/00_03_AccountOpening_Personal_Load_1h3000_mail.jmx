            <stringProp name="parameters"></stringProp>
            <stringProp name="script">// JSR223 Sampler (Groovy)

import javax.mail.Session
import javax.mail.Store
import javax.mail.Folder
import javax.mail.Message
import javax.mail.search.SubjectTerm
import javax.mail.internet.InternetAddress
import java.util.concurrent.TimeUnit // 時間単位を扱うためにインポート

// =========================================================
// 調整可能なパラメータ
// =========================================================
def MAIL_HOST = &quot;imap.gmail.com&quot; 
def MAIL_PORT = &quot;993&quot;           
def PROTOCOL = &quot;imaps&quot;          
def MAIL_FOLDER = &quot;INBOX&quot;       
def USERNAME = &quot;adamasst01&quot;     
def PASSWORD = &quot;rsvwjbrcxalfndij&quot; 

// CSVから取得されるメールアドレス (宛先チェック用)
def MY_EMAIL = vars.get(&quot;email&quot;) 
def EXPECTED_SUBJECT_PREFIX = &quot;【SBI VCトレード】登録メールアドレスを確認してください&quot; 

// 【ループ設定】
def MAX_MESSAGES_TO_CHECK = 25  // 1回の試行でチェックする最大件数 (元の50に戻しました)
def MAX_ATTEMPTS = 5            // 最大試行回数（50件チェックを最大5回繰り返す）
def WAIT_TIME_MS = 1000        // 1回の試行でメールが見つからなかった場合の待機時間 (10秒)

// =========================================================
// 実行ロジック
// =========================================================
log.info(&quot;自分のメールアドレス (検索キー): &quot; + MY_EMAIL);
log.info(&quot;期待する件名プレフィックス: &quot; + EXPECTED_SUBJECT_PREFIX);

// 初期設定
def props = new Properties()
props.setProperty(&quot;mail.store.protocol&quot;, PROTOCOL)
props.setProperty(&quot;mail.${PROTOCOL}.host&quot;, MAIL_HOST)
props.setProperty(&quot;mail.${PROTOCOL}.port&quot;, MAIL_PORT)
props.setProperty(&quot;mail.${PROTOCOL}.ssl.trust&quot;, &quot;*&quot;) 
props.setProperty(&quot;mail.${PROTOCOL}.ssl.checkserveridentity&quot;, &quot;false&quot;)
props.setProperty(&quot;mail.${PROTOCOL}.ssl.enable&quot;, &quot;true&quot;)
props.setProperty(&quot;mail.${PROTOCOL}.starttls.enable&quot;, &quot;true&quot;)

def session = Session.getDefaultInstance(props, null)
def store = null
def targetMessage = null
def attempts = 0
def extractedPath = null // 抽出したトークンを保持する変数を追加

// 【外部ループ】: 目的のメールが見つかるか、最大試行回数に達するまで繰り返す
// 注意: targetMessageの代わりに extractedPath が見つかるまでループするロジックに変更
while (extractedPath == null &amp;&amp; attempts &lt; MAX_ATTEMPTS) {
    attempts++
    log.info(&quot;--- メール検索試行 #${attempts}/${MAX_ATTEMPTS} を開始 (最大 ${MAX_MESSAGES_TO_CHECK} 通をチェック) ---&quot;);

    try {
        // 接続とフォルダオープン (試行ごとに再接続)
        store = session.getStore(PROTOCOL)
        store.connect(MAIL_HOST, USERNAME, PASSWORD)
        def folder = store.getFolder(MAIL_FOLDER)
        folder.open(Folder.READ_ONLY)

        def allMessages = folder.getMessages()
        log.info(&quot;メールボックスに ${allMessages.length} 通のメールが見つかりました。&quot;);

        // 内部ループ：最新のメールから順にチェック
        def start = allMessages.length - 1 
        def end = Math.max(0, allMessages.length - MAX_MESSAGES_TO_CHECK)

        for (int i = start; i &gt;= end; i--) {
            def message = allMessages[i]
            def subject = message.getSubject()
            def subjectMatch = (subject != null &amp;&amp; subject.startsWith(EXPECTED_SUBJECT_PREFIX))
            def toAddresses = message.getRecipients(Message.RecipientType.TO)
            def isToMyEmail = false
            
            if (toAddresses != null) {
                for (address in toAddresses) {
                    if (address.toString().contains(MY_EMAIL)) {
                        isToMyEmail = true
                        break
                    }
                }
            }
            
            if (subjectMatch &amp;&amp; isToMyEmail) {
                // targetMessage = message (Messageオブジェクトはそのまま保持)
                log.info(&quot;★目的のメールを発見しました！(試行 #${attempts}、インデックス: #${i+1})&quot;)

                // 【最重要修正点 A】: 接続が開いている間に、本文取得とトークン抽出を行う
                def content = message.getContent().toString()
                def regex = /\/signup\/set_password\/([a-zA-Z0-9_\-]+={0,2})/ 
                def matcher = (content =~ regex)

                if (matcher.find()) {
                    extractedPath = matcher.group(1) // 外部ループを抜けるために、外部変数に格納
                    log.info(&quot;抽出成功: passwordSetURL = &quot; + extractedPath)
                    
                    // JMeter変数に格納
                    vars.put(&quot;passwordSetURL&quot;, extractedPath)
                    
                    // ループを抜けるために内部ループもここで終了
                    break 
                } else {
                    log.warn(&quot;注意: メールは特定できましたが、トークンが見つかりませんでした。次のメールをチェックします。&quot;)
                }
            }
        }
        
        // フォルダとストアをクローズ（tryブロックの最後に配置）
        if (folder != null &amp;&amp; folder.isOpen()) {
            folder.close(false) 
        }

    } catch (Exception e) {
        log.error(&quot;メール接続または処理中に予期せぬエラーが発生しました: &quot; + e.getMessage(), e);
        // エラーが発生した場合も、finallyでストアをクローズ
    } finally {
        if (store != null &amp;&amp; store.isConnected()) {
            store.close()
        }
    }

    // メールが見つからなかった場合、待機する
    if (extractedPath == null &amp;&amp; attempts &lt; MAX_ATTEMPTS) {
        log.info(&quot;目的のメールは最新 ${MAX_MESSAGES_TO_CHECK} 通の中に見つかりませんでした。${WAIT_TIME_MS / 1000} 秒待機して再試行します。&quot;)
        TimeUnit.MILLISECONDS.sleep(WAIT_TIME_MS);
    }
}

// =========================================================
// 5. ループ後のサンプラー結果設定
// =========================================================

if (extractedPath != null) {
    // 【最重要修正点 B】: 結果設定をループの後に移動
    prev.setSuccessful(true)
    prev.setResponseData(&quot;トークン抽出成功: ${extractedPath}&quot;, &quot;UTF-8&quot;)
} else {
    // 目的のメールが最大試行回数をもってしても見つからなかった場合
    vars.put(&quot;passwordSetURL&quot;, &quot;EMAIL_NOT_FOUND_AFTER_MAX_ATTEMPTS&quot;)
    log.error(&quot;致命的なエラー: 最大試行回数 (${MAX_ATTEMPTS} 回) を超えても目的のメールが見つかりませんでした。&quot;)
    prev.setSuccessful(false)
    prev.setResponseMessage(&quot;最大試行回数を超えても目的のメールが見つかりませんでした。&quot;)
}</stringProp>
