              <stringProp name="script">// 1. このスレッドが使用しているメールアドレス変数 &apos;email&apos; を取得
def myEmail = vars.get(&quot;email&quot;); 
log.info(&quot;自分のメールアドレス (検索キー): &quot; + myEmail);

// 2. 親サンプラー(Mail Reader Sampler)が取得した全メールのリストを取得
def parentResult = prev;
def subResults = parentResult.getSubResults();

if (subResults == null || subResults.length == 0) {
    log.error(&quot;エラー: メールが1通も見つかりませんでした。&quot;);
    vars.put(&quot;passwordSetURL&quot;, &quot;NO_EMAIL_FOUND&quot;); 
    // メールがなければ、後続のパスワード設定は必ず失敗するので、サンプラー自体を失敗とします
    prev.setSuccessful(false); 
    prev.setResponseMessage(&quot;メールが見つかりませんでした。検索キー: &quot; + myEmail);
    return;
}

// 3. 全てのメールを逆順（最新から）にチェックし、自分のメールアドレスを含むメールを探す
def targetEmailResult = null;
for (int i = subResults.length - 1; i &gt;= 0; i--) {
    def emailResult = subResults[i];
    // メール本文全体を取得
    String emailBody = emailResult.getResponseDataAsString();
    
    // 4. 【自分のメール特定ロジック】メール本文に自分のメールアドレスが含まれているかチェック
    if (emailBody.contains(myEmail)) {
        log.info(&quot;★自分のメールが見つかりました: &quot; + emailResult.getSampleLabel() + &quot; に自分のアドレス (&quot; + myEmail + &quot;) が含まれています。&quot;);
        targetEmailResult = emailResult;
        break; // 見つけたらすぐにループを終了（最新のものを優先）
    }
}

// 5. 該当のメールが見つからなかった場合の処理
if (targetEmailResult == null) {
    log.error(&quot;エラー: 自分のメールアドレス (&quot; + myEmail + &quot;) を含むメールが見つかりませんでした。&quot;);
    vars.put(&quot;passwordSetURL&quot;, &quot;EMAIL_NOT_FOUND&quot;);
    prev.setSuccessful(false); 
    prev.setResponseMessage(&quot;自分のメールアドレスを含むメールが見つかりませんでした。&quot;);
    return;
}

// 6. 【トークン抽出】見つかったメールの本文を取得
String emailBody = targetEmailResult.getResponseDataAsString();

// 7. 【正規表現の修正】: URLセーフな文字（英数字、アンダーバー、ハイフン）と、末尾のイコール記号（=）を許容
// URL: https://s25-signup-ext-alb-114613775.ap-northeast-1.elb.amazonaws.com/signup/set_password/6sJGseF3f8igTS2WNWTw2mNqpvnsRnSVIoCUpQ8IqtlK_qfVcnaOn-alz40wE3SKD7vHzM9amA6UdY61ZMVztTMuEKfnktenQZn8YprIoFtwprL7ZHf4R8RLg_GrPzmtfUhuFOb2bJnCEeEvSOFw-GIQDWiPRNhpwmyR7RB9WEiqGru-2ggaW4i7i8NHm7stB4MAZ3MSpZzKLXY=
String regex = &quot;/signup/set_password/([a-zA-Z0-9_\\-]+={0,2})&quot;;

// 7. 正規表現でマッチングを実行
def matcher = (emailBody =~ regex);

// 8. マッチした場合、JMeter変数に保存
if (matcher.find()) {
    // matcher.group(1) はキャプチャグループ（トークン）の部分
    String extractedPath = matcher.group(1); 
    
    // &quot;passwordSetURL&quot; という名前のJMeter変数にトークン（パス）を保存
    vars.put(&quot;passwordSetURL&quot;, extractedPath);
    log.info(&quot;抽出成功: passwordSetURL = &quot; + extractedPath);
} else {
    log.error(&quot;エラー: 正規表現がメール本文にマッチしませんでした。Regex: &quot; + regex);
    vars.put(&quot;passwordSetURL&quot;, &quot;TOKEN_NOT_FOUND&quot;);
    prev.setSuccessful(false); 
    prev.setResponseMessage(&quot;メール本文からパスワード設定トークンが見つかりませんでした。&quot;);
}</stringProp>
