String accountNumber = vars.get(&quot;accountNumber&quot;);

// 4. 【NEW】レスポンスボディ(JSON)から &apos;hid&apos; を抽出
String responseBody = prev.getResponseDataAsString(); // 直前のレスポンスボディを取得
String hid = &quot;HID_NOT_FOUND&quot;; // デフォルト値

try {
    JsonSlurper slurper = new JsonSlurper();
    // レスポンスボディのテキストをJSONオブジェクトにパース
    def parsedJson = slurper.parseText(responseBody);
    
    // &apos;hid&apos; フィールドが存在すれば、その値を取得
    if (parsedJson.hid) {
        hid = parsedJson.hid;
    }
} catch (Exception e) {
    log.error(&quot;JSONのパースに失敗しました (&quot; + accountNumber + &quot;): &quot; + e.getMessage());
}

// 5. 直前のリクエストのレスポンスヘッダを取得
String responseHeaders = prev.getResponseHeaders(); // &apos;prev&apos;は直前のサンプラー結果

// 6. 正規表現で対象のCookieを抽出 (connect.sid=...;)
String regex = &quot;Set-Cookie: (&quot; + targetCookieName + &quot;=.*?);&quot;; 
java.util.regex.Matcher matcher = java.util.regex.Pattern.compile(regex, java.util.regex.Pattern.CASE_INSENSITIVE).matcher(responseHeaders);

String cookieInfo = &quot;COOKIE_NOT_FOUND&quot;;
if (matcher.find()) {
    // matcher.group(1) は ( ) で囲まれた部分 (例: &quot;connect.sid=s%3Aq9O0...kg&quot;)
    cookieInfo = matcher.group(1); 
}

// 7. CSVファイルへの書き込み
if (accountNumber != null &amp;&amp; !hid.equals(&quot;HID_NOT_FOUND&quot;) &amp;&amp; !cookieInfo.equals(&quot;COOKIE_NOT_FOUND&quot;)) {
    
    // 【NEW】書き込むヘッダ行
    String headerLine = &quot;accountNumber,hid,cookieInfo&quot;;
    // 【NEW】書き込むデータ行 (見やすいように変数名を変更)
    String dataLine = accountNumber + &quot;,&quot; + hid + &quot;,&quot; + cookieInfo + System.getProperty(&quot;line.separator&quot;);

    // スレッドセーフなファイル書き込みとヘッダ制御
    try {
        // this.getClass() をロックオブジェクトとして使い、排他制御する
        synchronized (this.getClass()) { 
            
            // ★ここから変更★
            File file = new File(outputFilePath);
            boolean fileExists = file.exists(); // FileWriterを開く前に存在チェック
            
            // &apos;true&apos; = 追記モードでファイルを開く
            java.io.FileWriter writer = new java.io.FileWriter(file, true); 
            
            // ファイルがまだ存在しなかった場合（＝最初の書き込みの場合）のみヘッダを書き込む
            if (!fileExists) {
                writer.write(headerLine + System.getProperty(&quot;line.separator&quot;));
            }
            
            // データ行を書き込む
            writer.write(dataLine);
            writer.close();
            // ★ここまで変更★
        }
    } catch (java.io.IOException e) {
        log.error(&quot;Cookie CSVの書き込みに失敗しました (&quot; + accountNumber + &quot;): &quot; + e.getMessage());
    }
    
} else {
    // ログに警告を出力 (JMeterのログウィンドウで確認)
    log.warn(&quot;口座番号、HID、またはCookieが見つかりません。 Account: &quot; + accountNumber + &quot;, HID: &quot; + hid + &quot;, Cookie Found: &quot; + (!cookieInfo.equals(&quot;COOKIE_NOT_FOUND&quot;)));
}</stringProp>
