            <stringProp name="script">import org.apache.jmeter.protocol.http.control.CookieManager
import org.apache.jmeter.protocol.http.control.Cookie

// 0. Cookie Managerを取得
CookieManager manager = sampler.getCookieManager();
if (manager == null) {
    log.warn(&quot;JSR223 PreProcessor: CookieManager が見つかりません。&quot;);
    return;
}

// 1. ★CSVから読み込んだ2つのCookie文字列を取得
String connectSidString = vars.get(&quot;connectSid&quot;);
String awsAlbString = vars.get(&quot;awsAlb&quot;);

if (connectSidString == null || awsAlbString == null) {
    log.warn(&quot;CSVから connectSid または awsAlb が読み込めませんでした。&quot;);
    return;
}

// 2. ★共通の Cookie 設定
String domain = sampler.getUrl().getHost();
String path = &quot;/&quot;;
boolean secure = sampler.getUrl().getProtocol().equalsIgnoreCase(&quot;https&quot;);
long expires = 0;

// 3. ★connect.sid を処理して Cookie Manager に追加
try {
    String[] partsSid = connectSidString.split(&quot;=&quot;, 2);
    String cookieNameSid = partsSid[0].trim();
    String cookieValueSid = partsSid[1].trim();
    
    Cookie cookieSid = new Cookie(cookieNameSid, cookieValueSid, domain, path, secure, expires);
    manager.add(cookieSid);
    // log.info(&quot;Added Cookie: &quot; + cookieNameSid);
} catch (Exception e) {
    log.error(&quot;connect.sid の処理に失敗: &quot; + e.getMessage());
}

// 4. ★awsAlb を処理して Cookie Manager に追加
try {
    String[] partsAlb = awsAlbString.split(&quot;=&quot;, 2);
    String cookieNameAlb = partsAlb[0].trim();
    String cookieValueAlb = partsAlb[1].trim();
    
    Cookie cookieAlb = new Cookie(cookieNameAlb, cookieValueAlb, domain, path, secure, expires);
    manager.add(cookieAlb);
    // log.info(&quot;Added Cookie: &quot; + cookieNameAlb);
} catch (Exception e) {
    log.error(&quot;awsAlb の処理に失敗: &quot; + e.getMessage());
}</stringProp>
