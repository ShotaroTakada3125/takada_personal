            <stringProp name="script">import org.apache.jmeter.protocol.http.control.CookieManager
import org.apache.jmeter.protocol.http.control.Cookie

// 1. CSVから読み込んだCookie文字列 (例: &quot;connect.sid=s%3A...&quot;)
// ※ CSV Data Set Config で &apos;cookieInfo&apos; という変数名で読み込んでいる前提
String cookieString = vars.get(&quot;cookieInfo&quot;);

if (cookieString == null || cookieString.isEmpty()) {
    log.warn(&quot;cookieInfo 変数がCSVから読み込めませんでした。&quot;);
    return; // 処理中断
}

// 2. 文字列を &quot;名前&quot; と &quot;値&quot; に分割
String[] parts = cookieString.split(&quot;=&quot;, 2);
if (parts.length &lt; 2) {
    log.warn(&quot;cookieInfo の形式が不正です: &quot; + cookieString);
    return; // 処理中断
}
String cookieName = parts[0].trim();
String cookieValue = parts[1].trim();

// 3. Cookie Managerを取得 (現在のサンプラーまたはスレッドグループのもの)
CookieManager manager = sampler.getCookieManager(); // &apos;sampler&apos;はこのPreProcessorが属するHTTPリクエスト

if (manager != null) {
    // 4. 新しいCookieオブジェクトを作成
    
    // Cookieが発行された際の Domain と Path を設定します。
    // sampler.getUrl().getHost() で、これからリクエストする先のホスト名 (ドメイン) を自動取得します。
    
    // ★★★ エラー箇所を修正 ★★★
    String domain = sampler.getUrl().getHost(); 
    
    // Pathは、ログイン時のSet-Cookieヘッダで &quot;Path=/&quot; だったため、&quot;/&quot; を指定します。
    String path = &quot;/&quot;; 
    
    // httpsならtrue, httpならfalse
    boolean secure = sampler.getUrl().getProtocol().equalsIgnoreCase(&quot;httpsS&quot;); 
    
    // 有効期限はCSVに保存していないため、0 (セッション扱い) にします
    long expires = 0; 
    
    Cookie cookie = new Cookie(cookieName, cookieValue, domain, path, secure, expires); 
    
    // 5. Cookie ManagerにCookieを追加 (同じ名前のCookieがあれば上書きされます)
    manager.add(cookie);
    
    // log.info(&quot;JSR223 PreProcessor: CookieをManagerに追加しました: &quot; + cookie.getName());
    
} else {
    log.warn(&quot;JSR223 PreProcessor: CookieManager が見つかりません。スレッドグループに HTTP Cookie Manager が有効か確認してください。&quot;);
}</stringProp>
